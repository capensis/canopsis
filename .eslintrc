{
    "rules": {
        "indent": [
            2,
            4,
            {"SwitchCase": 1}
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
	"no-console": 0
    },
    "env": {
        "browser": true
    },
    "globals": {
        "Ember": 1,
        "loader": 1,
        "DS": 1,
        "define": 1,
        "require": 1,
        "$": 1
    },
    "extends": "eslint:recommended"
}
