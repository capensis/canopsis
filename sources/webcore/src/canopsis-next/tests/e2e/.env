# Example for global chromedriver installed by yarn: /usr/local/share/.config/yarn/global/node_modules/chromedriver/lib/chromedriver/chromedriver
CHROME_DRIVER_PATH=

# Responsible for running the tests. If the flag is set to true, then tests run in parallel processes.
# true / false
TEST_WORKERS_ENABLED=false
# Count of concurrent processes
TEST_WORKERS_COUNT=2
# true / false
TEST_WORKERS_LIVE_OUTPUT_ENABLED=false

# true / false
TEST_VIDEOS_ENABLED=false
